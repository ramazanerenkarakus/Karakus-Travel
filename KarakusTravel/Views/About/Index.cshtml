@using KarakusTravel.Models.Siniflar
@model List<Hakkimizda>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımızda</h3>
            @foreach (var x in Model)
            {
                <img src="@x.FotoUrl" alt="" /> <!--Yol düzeltmesi yaptım-->
                <p style="font-family:sans-serif; font-size:30px;" >@x.Aciklama</p>
            }
        </div>
    </div>
</div>

